<a:Code>InsertChildParentExist</a:Code>
<a:Text>.FOREACH_PARENT()
/*  Parent &quot;%PARENT%&quot; must exist when inserting a child in &quot;%CHILD%&quot; */
if .JOIN(&quot;NEW(%.q:FK%) is not null&quot;, &quot;&quot;, &quot; and&quot;, &quot; then&quot;)
   select count(*)
   into   numrows
   from   %PARENT%
   where  .JOIN(&quot;%PK% = NEW(%.q:FK%)&quot;, &quot;and   &quot;,&quot;&quot;, &quot;;&quot;)

   if (numrows = 0) then
      .ERROR(-1002, &quot;Parent does not exist in &quot;%PARENT%&quot;. Cannot create child in &quot;%CHILD%&quot;.&quot;)
   end if;
end if;

.ENDFOR</a:Text>
