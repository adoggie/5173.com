<o:TextTargetItem Id="o110">
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, CONSTNAME ID, DELCONST ..., UPDCONST ...}

select
   u.usename,
   c.relname,
   t.tgconstrname,
   case p.proname
      when &#39;RI_FKey_cascade_del&#39; then &#39;cascade&#39;
      when &#39;RI_FKey_noaction_del&#39; then &#39;no action&#39;
      when &#39;RI_FKey_restrict_del&#39; then &#39;restrict&#39;
      when &#39;RI_FKey_setdefault_del&#39; then &#39;set default&#39;
      when &#39;RI_FKey_setnull_del&#39; then &#39;set null&#39;
      else &#39;&#39;
   end,
   case p.proname
      when &#39;RI_FKey_cascade_upd&#39; then &#39;cascade&#39;
      when &#39;RI_FKey_noaction_upd&#39; then &#39;no action&#39;
      when &#39;RI_FKey_restrict_upd&#39; then &#39;restrict&#39;
      when &#39;RI_FKey_setdefault_upd&#39; then &#39;set default&#39;
      when &#39;RI_FKey_setnull_upd&#39; then &#39;set null&#39;
      else &#39;&#39;
   end
from 
   pg_user u,
   pg_class c,
   pg_proc p,
   pg_trigger t
where 1=1
   and p.proname like &#39;RI_FKey_%&#39;
   and t.tgfoid=p.oid
   and t.tgconstrrelid=c.oid
   and c.relowner=u.usesysid
</a:TextTargetItem.Value>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
