<a:Text>.FOREACH_PARENT(&quot;FKMAXCARD&quot;)
/*  The cardinality of Parent &quot;%PARENT%&quot; in child &quot;%CHILD%&quot; cannot exceed %MAXCARD% */ 
if .JOIN(&quot;NEW(%FK%) is not null&quot;, &quot;&quot;, &quot; and&quot;, &quot; then&quot;)
   select count(*)
   into cardinality
   from   %CHILD% 
   where .JOIN(&quot;%FK% = NEW(%FK%)&quot;, &quot;and   &quot;, &quot;&quot;, &quot;;&quot;)  
   if (cardinality &gt;= %MAXCARD%) then
      .ERROR(-1007, &quot;Child Maximum cardinality exceeded! Cannot create child in &quot;%CHILD%&quot;.&quot;)
   end if;
end if;
.ENDFOR</a:Text>
