   u.usename,
   t.relname,
   i.relname,
   case (x.indisunique)
      when true then &#39;unique&#39; else &#39;&#39;
   end,
   a.attname || &#39;,&#39;
from
   pg_user u,
   pg_class t,
   pg_class i,
   pg_index x,
   pg_attribute a
where 1=1
   and t.relowner=u.usesysid
